			drop_button.Size = UDim2.new(0, 41, 0, 40)
			drop_button.Name = "DropButton"
			drop_button.Parent = drop
			
			for i, v in pairs(DropDownInfo.Options) do
				
				local drop_btn = Instance.new("TextButton")
				drop_btn.Font = Enum.Font.GothamBold
				drop_btn.Text = v
				drop_btn.TextColor3 = Color3.new(0.933333, 0.933333, 0.933333)
				drop_btn.TextScaled = true
				drop_btn.TextSize = 14
				drop_btn.TextWrapped = true
				drop_btn.AnchorPoint = Vector2.new(0.5, 0.5)
				drop_btn.BackgroundColor3 = Color3.new(0.278431, 0.278431, 0.278431)
				drop_btn.Size = UDim2.new(0, 317, 0, 41)
				drop_btn.Name = "DropBtn"
				drop_btn.Parent = dropdown_buttons

				local uicorner_2 = Instance.new("UICorner")
				uicorner_2.Parent = drop_btn

				local uistroke_2 = Instance.new("UIStroke")
				uistroke_2.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
				uistroke_2.Color = Color3.new(0.568627, 0.568627, 0.568627)
				uistroke_2.Thickness = 1.2000000476837158
				uistroke_2.Parent = drop_btn

				local uipadding_2 = Instance.new("UIPadding")
				uipadding_2.PaddingBottom = UDim.new(0, 7)
				uipadding_2.PaddingLeft = UDim.new(0, 7)
				uipadding_2.PaddingRight = UDim.new(0, 7)
				uipadding_2.PaddingTop = UDim.new(0, 7)
				uipadding_2.Parent = drop_btn
				
				drop_btn.MouseButton1Down:Connect(function()
					CB(v)
					dropdown_buttons.Visible = false
					game.TweenService:Create(drop_image, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Rotation = 0,}):Play()
					game.TweenService:Create(drop, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Size = UDim2.new(0, 345, 0, 50)}):Play()
					game.TweenService:Create(labeltext, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Size = UDim2.new(0.692753613, 0, 0.429106861, 0)}):Play()
					game.TweenService:Create(drop_image, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Position = UDim2.new(0.837681174, 0, 0.080758974, 0)}):Play()
					game.TweenService:Create(drop_button, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Position = UDim2.new(0.837681174, 0, 0.080758974, 0)}):Play()
				
					
				end)
				
				
					
			end
			
			local DropOpen = false
			
			drop_button.MouseButton1Down:Connect(function()
				--print("watafak")
				if DropOpen then
					
					dropdown_buttons.Visible = false
					game.TweenService:Create(drop_image, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Rotation = 0,}):Play()
					game.TweenService:Create(drop, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Size = UDim2.new(0, 345, 0, 50)}):Play()
					game.TweenService:Create(labeltext, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Size = UDim2.new(0.692753613, 0, 0.429106861, 0)}):Play()
					game.TweenService:Create(drop_image, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Position = UDim2.new(0.837681174, 0, 0.080758974, 0)}):Play()
					game.TweenService:Create(drop_button, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Position = UDim2.new(0.837681174, 0, 0.080758974, 0)}):Play()
					wait(.4)
					DropOpen = false
				elseif not DropOpen then
				
					game.TweenService:Create(drop_image, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Rotation = 180,}):Play()
					game.TweenService:Create(drop, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Size = UDim2.new(0, 345, 0, 141)}):Play()
					game.TweenService:Create(labeltext, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Size = UDim2.new(0.693, 0,0.138, 0)}):Play()
					game.TweenService:Create(drop_image, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Position = UDim2.new(0.835, 0,0.019, 0)}):Play()
					game.TweenService:Create(drop_button, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Position = UDim2.new(0.835, 0,0.019, 0)}):Play()
					wait(.4)
					dropdown_buttons.Visible = true
					DropOpen = true
				end
			end)
			
			
				
		
			
			
		end
		
		function Elements:NewKeybind(KeyBindInfo)
			local CB = KeyBindInfo.Callback or function() end
			local key_bind = Instance.new("Frame")
			key_bind.BackgroundColor3 = Color3.new(1, 1, 1)
			key_bind.Position = UDim2.new(0.503292501, 0, 0.519230545, 0)
			key_bind.Size = UDim2.new(0, 345, 0, 50)
			key_bind.Name = "KeyBind"
			key_bind.Parent = example_tab

			local uistroke = Instance.new("UIStroke")
			uistroke.Color = Color3.new(0.494118, 0.494118, 0.494118)
			uistroke.Thickness = 1.2000000476837158
			uistroke.Parent = key_bind

			local uigradient = Instance.new("UIGradient")
			uigradient.Color = ColorSequence.new({ColorSequenceKeypoint.new(0, Color3.new(0.164706, 0.164706, 0.164706)), ColorSequenceKeypoint.new(1, Color3.new(0.223529, 0.223529, 0.223529))})
			uigradient.Rotation = 220
			uigradient.Parent = key_bind

			local uicorner = Instance.new("UICorner")
			uicorner.Parent = key_bind

			local labeltext = Instance.new("TextLabel")
			labeltext.Font = Enum.Font.GothamBold
			labeltext.Text = KeyBindInfo.KeyBindName
			labeltext.TextColor3 = Color3.new(0.933333, 0.933333, 0.933333)
			labeltext.TextScaled = true
			labeltext.TextSize = 14
			labeltext.TextWrapped = true
			labeltext.TextXAlignment = Enum.TextXAlignment.Left
			labeltext.BackgroundColor3 = Color3.new(1, 1, 1)
			labeltext.BackgroundTransparency = 1
			labeltext.BorderSizePixel = 0
			labeltext.Position = UDim2.new(0, 8, 0, 14)
			labeltext.Size = UDim2.new(0.518840551, 0, 0.429106861, 0)
			labeltext.ZIndex = 11
			labeltext.Name = "Labeltext"
			labeltext.Parent = key_bind

			local search = Instance.new("Frame")
			search.BackgroundColor3 = Color3.new(1, 1, 1)
			search.BackgroundTransparency = 1
			search.BorderSizePixel = 0
			search.Position = UDim2.new(0.68602401, 0, 0.0234735105, 0)
			search.Size = UDim2.new(0.304268897, 0, 0.408855557, 0)
			search.ZIndex = 11
			search.Name = "Search"
			search.Parent = key_bind

			local bg = Instance.new("Frame")
			bg.AnchorPoint = Vector2.new(0.5, 0.5)
			bg.BackgroundColor3 = Color3.new(0.227451, 0.227451, 0.227451)
			bg.BorderSizePixel = 0
			bg.Position = UDim2.new(0.572109997, 0, 1.14588988, 0)
			bg.Size = UDim2.new(0.648854256, 0, 1.60693932, 0)
			bg.ZIndex = 11
			bg.Name = "BG"
			bg.Parent = search

			local uicorner_2 = Instance.new("UICorner")
			uicorner_2.CornerRadius = UDim.new(0, 5)
			uicorner_2.Parent = bg

			local kbset = Instance.new("TextButton")
			kbset.Font = Enum.Font.SourceSans
			kbset.Text = ""
			kbset.TextColor3 = Color3.new(0, 0, 0)
			kbset.TextSize = 14
			kbset.AnchorPoint = Vector2.new(0.5, 0.5)
			kbset.BackgroundColor3 = Color3.new(1, 1, 1)
			kbset.BackgroundTransparency = 1
			kbset.Position = UDim2.new(0.5, 0, 0.484575629, 0)
			kbset.Size = UDim2.new(0.999170601, 0, 0.97452271, 0)
			kbset.ZIndex = 20
			kbset.Name = "KBSet"
			kbset.Parent = bg

			local icon = Instance.new("ImageButton")
			icon.Image = "rbxassetid://3926305904"
			icon.ImageRectOffset = Vector2.new(724, 444)
			icon.ImageRectSize = Vector2.new(36, 36)
			icon.AnchorPoint = Vector2.new(0.5, 0.5)
			icon.BackgroundTransparency = 1
			icon.BorderSizePixel = 0
			icon.Position = UDim2.new(0.433552086, 0, 1.10005856, 0)
			icon.Size = UDim2.new(0.209255174, 0, 1.14469612, 0)
			icon.ZIndex = 12
			icon.Name = "Icon"
			icon.Parent = search

			local select_key = Instance.new("TextLabel")
			select_key.Font = Enum.Font.GothamMedium
			select_key.Text = "Please Select A Key."
			select_key.TextColor3 = Color3.new(0.933333, 0.933333, 0.933333)
			select_key.TextScaled = true
			select_key.TextSize = 14
			select_key.TextTransparency = 1
			select_key.TextWrapped = true
			select_key.BackgroundColor3 = Color3.new(1, 1, 1)
			select_key.BackgroundTransparency = 1
			select_key.BorderSizePixel = 0
			select_key.Position = UDim2.new(-0.0308999401, 0, 0.676342726, 0)
			select_key.Size = UDim2.new(0.834431231, 0, 0.994598687, 0)
			select_key.ZIndex = 30
			select_key.Name = "SelectKey"
			select_key.Parent = search

			local key = Instance.new("TextLabel")
			key.Font = Enum.Font.GothamMedium
			key.Text = UIS:GetStringForKeyCode(KeyBindInfo.Defaultkeybind)
			key.TextColor3 = Color3.new(1, 1, 1)
			key.TextScaled = true
			key.TextSize = 14
			key.TextTransparency = 0.6000000238418579
			key.TextWrapped = true
			key.BackgroundColor3 = Color3.new(1, 1, 1)
			key.BackgroundTransparency = 1
			key.BorderSizePixel = 0
			key.Position = UDim2.new(0.597833931, 0, 0.52959162, 0)
			key.Size = UDim2.new(0.205697566, 0, 0.994598687, 0)
			key.ZIndex = 30
			key.Name = "Key"
			key.Parent = search
			
			local Currentkeybind = KeyBindInfo.Defaultkeybind
			local SettingKeyBind = false
			
			kbset.MouseButton1Down:Connect(function()
				if not SettingKeyBind then
				game.TweenService:Create(key, 

					TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

					{TextTransparency = 1}):Play()
				game.TweenService:Create(bg, 

					TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Size = UDim2.new(1.287, 0,1.607, 0),Position = UDim2.new(0.253, 0,1.146, 0)}):Play()
				game.TweenService:Create(icon, 

					TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

					{Position = UDim2.new(-0.224, 0,1.146, 0)}):Play()
				wait(.4)
				game.TweenService:Create(select_key, 

					TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{TextTransparency = 0}):Play()
					wait(.4)
					SettingKeyBind = true
					end
			end)
			UIS.InputBegan:Connect(function(KeyCode)
				if SettingKeyBind then
				if KeyCode.KeyCode == Enum.KeyCode.Unknown then
					select_key.Text = "Invalid Key."
					wait(.4)
					select_key.Text = "Please Select A Key."
				else
						Currentkeybind = KeyCode.KeyCode
						key.Text = UIS:GetStringForKeyCode(KeyCode.KeyCode)
					
						game.TweenService:Create(select_key, 

							TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

							{TextTransparency = 1}):Play()
						wait(.24)
						game.TweenService:Create(key, 

							TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

							{TextTransparency = 0}):Play()
						game.TweenService:Create(bg, 

							TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

							{Size = UDim2.new(0.649, 0,1.607, 0),Position = UDim2.new(0.572109997, 0, 1.14588988, 0)}):Play()
						game.TweenService:Create(icon, 

							TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

							{Position = UDim2.new(0.434, 0,1.1, 0)}):Play()
						
						wait(.4)
						SettingKeyBind = false
					end
				end
				if KeyCode.KeyCode == Currentkeybind and not SettingKeyBind then
					CB(Currentkeybind)
					
				end
			end)
		end
		
		function Elements:NewLabel(LabelText)
			local label = Instance.new("Frame")
			label.BackgroundColor3 = Color3.new(1, 1, 1)
			label.Position = UDim2.new(0.0109848278, 0, 0.010603331, 0)
			label.Size = UDim2.new(0, 345, 0, 50)
			label.Name = "Label"
			label.Parent = example_tab

			local uistroke = Instance.new("UIStroke")
			uistroke.Color = Color3.new(0.494118, 0.494118, 0.494118)
			uistroke.Thickness = 1.2000000476837158
			uistroke.Parent = label

			local uigradient = Instance.new("UIGradient")
			uigradient.Color = ColorSequence.new({ColorSequenceKeypoint.new(0, Color3.new(0.164706, 0.164706, 0.164706)), ColorSequenceKeypoint.new(1, Color3.new(0.223529, 0.223529, 0.223529))})
			uigradient.Rotation = 220
			uigradient.Parent = label

			local uicorner = Instance.new("UICorner")
			uicorner.Parent = label

			local labeltext = Instance.new("TextLabel")
			labeltext.Font = Enum.Font.GothamBold
			labeltext.Text = LabelText
			labeltext.TextColor3 = Color3.new(0.933333, 0.933333, 0.933333)
			labeltext.TextScaled = true
			labeltext.TextSize = 14
			labeltext.TextWrapped = true
			labeltext.TextXAlignment = Enum.TextXAlignment.Left
			labeltext.BackgroundColor3 = Color3.new(1, 1, 1)
			labeltext.BackgroundTransparency = 1
			labeltext.BorderSizePixel = 0
			labeltext.Position = UDim2.new(0, 8, 0, 8)
			labeltext.Size = UDim2.new(0.953623176, 0, 0.649106801, 0)
			labeltext.ZIndex = 11
			labeltext.Name = "Labeltext"
			labeltext.Parent = label
		end
		
