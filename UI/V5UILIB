			slider_bar.BackgroundColor3 = Color3.new(0.156863, 0.407843, 0.607843)
			slider_bar.BackgroundTransparency = 0.800000011920929
			slider_bar.BorderSizePixel = 0
			slider_bar.Position = UDim2.new(0.472496331, 0, 0.1590271, 0)
			slider_bar.Size = UDim2.new(0.504315257, 0, 0.66600275, 0)
			slider_bar.ZIndex = 11
			slider_bar.Name = "SliderBar"
			slider_bar.Parent = slider

			local uicorner_2 = Instance.new("UICorner")
			uicorner_2.CornerRadius = UDim.new(0, 6)
			uicorner_2.Parent = slider_bar

			local action = Instance.new("TextButton")
			action.Font = Enum.Font.SourceSans
			action.Text = ""
			action.TextColor3 = Color3.new(0, 0, 0)
			action.TextSize = 14
			action.BackgroundColor3 = Color3.new(1, 1, 1)
			action.BackgroundTransparency = 1
			action.BorderSizePixel = 0
			action.Size = UDim2.new(1, 0, 1, 0)
			action.ZIndex = 25
			action.Name = "Action"
			action.Parent = slider_bar

			local amount = Instance.new("Frame")
			amount.BackgroundColor3 = Color3.new(0.184314, 0.470588, 0.701961)
			amount.BorderSizePixel = 0
			amount.Size = UDim2.new(0, 0, 1, 0)
			amount.ZIndex = 11
			amount.Name = "Amount"
			amount.Parent = action

			local uicorner_3 = Instance.new("UICorner")
			uicorner_3.CornerRadius = UDim.new(0, 6)
			uicorner_3.Parent = amount

			local uistroke_2 = Instance.new("UIStroke")
			uistroke_2.Color = Color3.new(0.160784, 0.368627, 0.560784)
			uistroke_2.Thickness = 1.5
			uistroke_2.Parent = amount

			local uigradient_2 = Instance.new("UIGradient")
			uigradient_2.Color = ColorSequence.new({ColorSequenceKeypoint.new(0, Color3.new(0.658824, 0.658824, 0.658824)), ColorSequenceKeypoint.new(1, Color3.new(1, 1, 1))})
			uigradient_2.Rotation = -90
			uigradient_2.Parent = amount

			local uistroke_3 = Instance.new("UIStroke")
			uistroke_3.Color = Color3.new(0.160784, 0.368627, 0.560784)
			uistroke_3.Thickness = 1.5
			uistroke_3.Parent = slider_bar

			local title = Instance.new("TextLabel")
			title.Font = Enum.Font.GothamMedium
			title.Text = tostring(SliderInfo.SliderText).." - "..tostring(SliderInfo.MinimumAmount)
			title.TextColor3 = Color3.new(1, 1, 1)
			title.TextScaled = true
			title.TextSize = 14
			title.TextTransparency = 0.2199999988079071
			title.TextWrapped = true
			title.TextXAlignment = Enum.TextXAlignment.Left
			title.BackgroundColor3 = Color3.new(1, 1, 1)
			title.BackgroundTransparency = 1
			title.BorderSizePixel = 0
			title.Position = UDim2.new(0.0738896504, 0, 0.204674706, 0)
			title.Size = UDim2.new(0.758220971, 0, 0.577376902, 0)
			title.ZIndex = 30
			title.Name = "Title"
			title.Parent = slider_bar

			local uigradient_3 = Instance.new("UIGradient")
			uigradient_3.Color = ColorSequence.new({ColorSequenceKeypoint.new(0, Color3.new(0.658824, 0.658824, 0.658824)), ColorSequenceKeypoint.new(1, Color3.new(0, 1, 1))})
			uigradient_3.Rotation = -90
			uigradient_3.Parent = slider_bar
			
			local Mouse = game:GetService("Players").LocalPlayer:GetMouse()
			local Slider = slider
			local SliderFrame = amount
			local Button = action
			local UIS = game:GetService("UserInputService")


		

			local holding = false

			Button.MouseButton1Down:Connect(function()
				holding = true
				
			end)

			Button.MouseButton1Up:Connect(function()
				if not holding then return end
				holding = false
				
			end)

			UIS.InputEnded:Connect(function(Input)
				if Input.UserInputType == Enum.UserInputType.MouseButton1 then
					if not holding then return end
					holding = false
					
				end
			end)
			
			game:GetService("RunService").RenderStepped:Connect(function()
				if holding then
					local Percentage = math.clamp((Mouse.X - amount.AbsolutePosition.X) / amount.AbsoluteSize.X,0,1)
					local Value = math.floor((SliderInfo.MaximumAmount - SliderInfo.MinimumAmount) * Percentage) + SliderInfo.MinimumAmount
					game.TweenService:Create(SliderFrame, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
						Size = UDim2.new(Percentage, 0, 1, 0)
					}):Play()
					--SliderFrame.Size = UDim2.new(Percentage, 0, 0, 8)
					title.Text = tostring(SliderInfo.SliderText).." - "..tostring(Value)
					CB(Value, math.round(Value))
				end
			end)
			
			
			
		end
		
