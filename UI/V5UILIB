	local Tabs = {}

	function Tabs:NewTab(TabName)
		
		local example_tab = Instance.new("ScrollingFrame")
		example_tab.AutomaticCanvasSize = Enum.AutomaticSize.Y
		example_tab.BottomImage = "rbxassetid://158362221"
		example_tab.CanvasSize = UDim2.new(0, 0, 0, 0)
		example_tab.MidImage = "rbxassetid://158362264"
		example_tab.ScrollBarThickness = 0
		example_tab.TopImage = "rbxassetid://158362307"
		example_tab.Active = true
		example_tab.BackgroundColor3 = Color3.new(1, 1, 1)
		example_tab.BackgroundTransparency = 1
		example_tab.BorderSizePixel = 0
		example_tab.Position = UDim2.new(0.331383556, 0, 0.0949720666, 0)
		example_tab.Size = UDim2.new(0, 370, 0, 313)
		example_tab.Name = "Tab"
		example_tab.Parent = hex_lib
		example_tab.Visible = false
		
		local uipadding_2 = Instance.new("UIPadding")
		uipadding_2.PaddingLeft = UDim.new(0, 4)
		uipadding_2.PaddingTop = UDim.new(0, 7)
		uipadding_2.Parent = example_tab

		local uilist_layout_2 = Instance.new("UIListLayout")
		uilist_layout_2.Padding = UDim.new(0, 6)
		uilist_layout_2.SortOrder = Enum.SortOrder.LayoutOrder
		uilist_layout_2.Parent = example_tab

		local tabBtN = Instance.new("TextButton")
		tabBtN.Font = Enum.Font.GothamBold
		tabBtN.Text = TabName
		tabBtN.TextColor3 = Color3.new(0.933333, 0.933333, 0.933333)
		tabBtN.TextScaled = true
		tabBtN.TextSize = 14
		tabBtN.TextWrapped = true
		tabBtN.BackgroundColor3 = Color3.new(0.278431, 0.278431, 0.278431)
		tabBtN.Position = UDim2.new(0, 0, 0.205128208, 0)
		tabBtN.Size = UDim2.new(0, 125, 0, 41)
		tabBtN.Name = "TabBTN"
		tabBtN.Parent = buttons_tabs

		local uicorner = Instance.new("UICorner")
		uicorner.Parent = tabBtN

		local uistroke = Instance.new("UIStroke")
		uistroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
		uistroke.Color = Color3.new(0.568627, 0.568627, 0.568627)
		uistroke.Thickness = 1.2000000476837158
		uistroke.Parent = tabBtN

		local uipadding = Instance.new("UIPadding")
		uipadding.PaddingBottom = UDim.new(0, 7)
		uipadding.PaddingLeft = UDim.new(0, 7)
		uipadding.PaddingRight = UDim.new(0, 7)
		uipadding.PaddingTop = UDim.new(0, 7)
		uipadding.Parent = tabBtN


		tabBtN.MouseButton1Down:Connect(function()
		
			for i,v in next, hex_lib:GetChildren() do
				if v:IsA("UICorner") or v:IsA("UIScale") or v:IsA("UIListLayout") or v:IsA("UIStroke") or v:IsA("UIGradient") then 

					print("Attempted To Close A Non Closeable Item (Normal Error)")
				else
					if v.Name == "Tab" then
						
					v.Visible = false	
                  end
				end


			end
			for i,v in next, buttons_tabs:GetChildren() do
				if v:IsA("UICorner") or v:IsA("UIScale") or v:IsA("UIListLayout") or v:IsA("UIGradient") then 

					print("Attempted To Color A Non Closeable Item (Normal Error)")
				else
					if v.Name == "TabBTN" then
						game.TweenService:Create(

							v, 

							TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

							{BackgroundColor3 = Color3.fromRGB(71, 71, 71)}

						):Play()
						game.TweenService:Create(

							v.UIStroke, 

							TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

							{Color = Color3.fromRGB(145, 145, 145)}

						):Play()
						
					end
				end


			end
			example_tab.Visible = true
			game.TweenService:Create(

				tabBtN, 

				TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

				{BackgroundColor3 = Color3.fromRGB(0, 48, 35)}

			):Play()
			game.TweenService:Create(

				tabBtN.UIStroke, 

				TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

				{Color = Color3.fromRGB(0, 125, 89)}

			):Play()
			
			
			
			
		end)

			local Elements = {}

			function Elements:NewSlider(SliderInfo)
				local CB = SliderInfo.Callback or function() end
			
			local slider = Instance.new("Frame")
			slider.BackgroundColor3 = Color3.new(1, 1, 1)
			slider.Position = UDim2.new(0.0109848278, 0, 0.134255543, 0)
