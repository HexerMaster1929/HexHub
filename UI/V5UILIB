	uigradient.Rotation = 220
	uigradient.Parent = notification

	local uistroke = Instance.new("UIStroke")
	uistroke.Color = Color3.new(0, 0.258824, 0.188235)
	uistroke.Thickness = 1.2000000476837158
	uistroke.Parent = notification
	uistroke.Transparency = 1

	local dsc = Instance.new("TextLabel")
	dsc.Font = Enum.Font.GothamBold
	dsc.Text = NotificationInfo.Desc
	dsc.TextColor3 = Color3.new(0.933333, 0.933333, 0.933333)
	dsc.TextScaled = true
	dsc.TextSize = 14
	dsc.TextWrapped = true
	dsc.TextXAlignment = Enum.TextXAlignment.Left
	dsc.BackgroundColor3 = Color3.new(1, 1, 1)
	dsc.BackgroundTransparency = 1
	dsc.BorderSizePixel = 0
	dsc.Position = UDim2.new(0.0261262357, 0, 0.395263672, 0)
	dsc.Size = UDim2.new(0.949045777, 0, 0.516498625, 0)
	dsc.ZIndex = 11
	dsc.Name = "Dsc"
	dsc.Parent = notification
	dsc.TextTransparency = 1

	wait()
	ButtonSFX()
	game.TweenService:Create(notification.UIStroke, TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 0,}):Play()
	game.TweenService:Create(notification, TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0,}):Play()
	game.TweenService:Create(tit, TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0,}):Play()
	game.TweenService:Create(dsc, TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0,}):Play()
	game.TweenService:Create(ico, TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {ImageTransparency = 0,}):Play()
	
	wait(NotificationInfo.Time)
	game.TweenService:Create(notification.UIStroke, TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 1,}):Play()
	game.TweenService:Create(notification, TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 1,}):Play()
	game.TweenService:Create(tit, TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1,}):Play()
	game.TweenService:Create(dsc, TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1,}):Play()
	game.TweenService:Create(ico, TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {ImageTransparency = 1,}):Play()
	wait(.4)
	notification:Destroy()
end

