						tog, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{BackgroundColor3 = Color3.fromRGB(0, 48, 35)}

					):Play()
					game.TweenService:Create(

						uistroke_2, 

						TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), 

						{Color = Color3.fromRGB(0, 125, 89)}

					):Play()
					CB(true)
					IsToggled = true
				end
			end)

			
			

		end
		
